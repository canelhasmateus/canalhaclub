{
	"name": "Jewel",
	"type": "dark",
	"colors": {
		"editor.background": "#2B2B2B",
		"editor.foreground": "#A9B7C6",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb",
	},
	"tokenColors": [
		{
			"name": "Source",
			"scope": [
				"source",
			],
			"settings": {
				"foreground": "#A9B7C6",
			}
		},
		{
			"name": "Comment",
			"scope": [
				"punctuation.definition.comment",
				"comment.line",
			],
			"settings": {
				"foreground": "#808080",
			}
		},
		{
			"name": "Escape",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#FFC66D",
			}
		},
		{
			"name": "Imports",
			"scope": [
				"meta.use.rust",
				"storage.modifier.import",
				"storage.modifier.package"
			],
			"settings": {
				"foreground": "#A9B7C6",
			}
		},
		{
			"name": "Declarations",
			"scope": [
				"meta.function.definition entity.name.function",
				"entity.name.module",
				"entity.name.function.julia",
			],
			"settings": {
				"foreground": "#FFC66D"
			}
		},

		{
			"name": "Keyword",
			"scope": [
				"entity.name.label",
				// punctuation.definition.variable.svelte
				// "entity.other.inherited-class",
				"keyword",
				"punctuation",
				"storage.type",
				"storage.modifier",
				"variable.language.self",
				"meta.bracket.julia",
				"constant.language",
			],
			"settings": {
				"foreground": "#CC7832",
			}
		},
		{
			"name": "Soft Operations",
			"scope": [
				"keyword.operator",
			],
			"settings": {
				"foreground": "#e67d7d"
			}
		},
		{
			"name": "Text",
			"scope": [
				"string.quoted",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#629755"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#6897BB",
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"entity.name.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Meta",
			"scope": [
				"entity.name.function.macro",
				"meta.attribute",
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Macros",
			"scope": [
				"support.function.macro",
				"punctuation.definition.annotation",
				"storage.type.annotation"
			],
			"settings": {
				"foreground": "#98a12a",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Types",
			"scope": [
				"support.type",
				"meta.type",
				"storage.type.concrete",
				"storage.type.primitive",
				"storage.type.built-in",
				"entity.other.inherited-class",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#D1DDE9",
				"fontStyle": "italic bold",
			}
		},
		// Typescript
		{
			"name": "Object Literal",
			"scope": [
				"meta.objectliteral.ts",
			],
			"settings": {
				"foreground": "#9876AA",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Object Literal",
			"scope": [
				"meta.object-literal.key.ts",
				"meta.objectliteral.ts punctuation.definition.block.ts"
			],
			"settings": {
				"foreground": "#9876AA",
				"fontStyle": "underline"
			}
		},
		
		{
			"scope": [
				"meta.definition.function.ts",
				"punctuation.definition.string.template.begin.ts",
				"punctuation.definition.string.template.end.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.begin.ts"
			],
			"settings": {
				"foreground": "#FFC66D"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"meta.import.ts meta.block.ts",
				"meta.import.ts variable.other.readwrite.alias.ts"
			],
			"settings": {
				"foreground": "#82AAFF",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"keyword.operator.assignment.ts"
			],
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"name": "call",
			"scope": [
				"meta.function-call.ts entity.name.function.ts"
			],
			"settings": {
				"foreground": "#82AAFF",
			}
		},
		{
			"scope": "variable.other.constant.ts",
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"scope": "string.template.ts",
			"settings": {
				"foreground": "#629755"
			}
		},
		{
			"name": "",
			"scope": [
				"string.template.ts meta.template.expression.ts meta.embedded.line.ts variable.other.readwrite.ts",
				"string.template.ts meta.template.expression.ts meta.embedded.line.ts punctuation.accessor.ts",
				"string.template.ts meta.template.expression.ts meta.embedded.line.ts variable.other.object.ts",
				"string.template.ts meta.template.expression.ts meta.embedded.line.ts variable.other.property.ts",
			],
			"settings": {
				"foreground": "#A9B7C6",
				"fontStyle": "underline"
			}
		},
		//python
		{
			"scope": "support.function.magic.python",
			"settings": {
				"foreground": "#9876AA",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "meta.function-call.arguments.python",
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"scope": "constant.language.python",
			"settings": {
				"foreground": "#CC7832"
			}
		},
		// nim 
		{
			"scope": [
				"support.constant.nim"
			],
			"settings": {
				"foreground": "#9876AA",
				"fontStyle": "italic bold",
			}
		},
		{
			"scope": "entity.name.function.nim",
			"settings": {
				"foreground": "#FFC66D"
			}
		},
		{
			"scope": "support.function.any-method.nim",
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"scope": "constant.language.nim",
			"settings": {
				"foreground": "#CC7832"
			}
		},
		// julia
		{
			"scope": "support.function.julia",
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"scope": [
				"storage.modifier.variable.julia",
				"constant.language.julia"
			],
			"settings": {
				"foreground": "#CC7832"
			}
		},
		// clojure
		{
			"scope": "variable.other.constant.clojure",
			"settings": {
				"foreground": "#FFC66D"
			}
		},
		{
			"scope": "meta.symbol.namespace",
			"settings": {
				"foreground": "#4f95ce"
			}
		},
		{
			"scope": "entity.name.function.clojure",
			"settings": {
				"foreground": "#82AAFF"
			}
		},
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"method.declaration:java": {
			"foreground": "#FFC66D",
		},
		"*.typeHint:python": {
			"foreground": "#D1DDE9",
			"fontStyle": "italic bold"
		},
		"selfParameter:python": {
			"foreground": "#9876AA",
			"fontStyle": "italic"
		},
		"parameter:python": {
			"foreground": "#A9B7C6",
		},
		"variable:python": {
			"foreground": "#A9B7C6",
		},
		"function.declaration:python": {
			"foreground": "#FFC66D",
		},
		"method.declaration:python": {
			"foreground": "#FFC66D",
		},
	}
}
